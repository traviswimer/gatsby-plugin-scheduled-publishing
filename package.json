{
	"name": "gatsby-plugin-mdx-scheduled-publishing",
	"version": "1.0.0",
	"description": "Prevents MDX pages from being included until a specified publish date has passed.",
	"license": "MIT",
	"keywords": [
		"gatsby",
		"gatsby-plugin",
		"markdown",
		"scheduling",
		"blog"
	],
	"peerDependencies": {
		"gatsby": "^4.0.0-next"
	},
	"scripts": {
		"build": "tsc --project .",
		"typecheck": "tsc --project . --noEmit",
		"start": "tsc --project . -w",
		"test": "jest --coverage --watchAll",
		"ci_test": "jest --coverage --ci",
		"prepublishOnly": "yarn build"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/traviswimer/gatsby-plugin-mdx-scheduled-publishing.git"
	},
	"bugs": {
		"url": "https://github.com/traviswimer/gatsby-plugin-mdx-scheduled-publishing/issues"
	},
	"homepage": "https://github.com/traviswimer/gatsby-plugin-mdx-scheduled-publishing",
	"contributors": [
		{
			"name": "Travis Wimer",
			"url": "https://traviswimer.com"
		}
	],
	"devDependencies": {
		"@babel/core": "^7.18.10",
		"@babel/preset-env": "^7.18.10",
		"@babel/preset-typescript": "^7.18.6",
		"@types/jest": "^28.1.6",
		"@types/luxon": "^3.0.0",
		"@types/node": "^18.6.4",
		"babel-jest": "^28.1.3",
		"gatsby": "^4.20.0",
		"jest": "^28.1.3",
		"ts-jest": "^28.0.7",
		"ts-node": "^10.9.1",
		"typescript": "^4.7.4"
	},
	"dependencies": {
		"lodash": "^4.17.21",
		"luxon": "^3.0.1"
	}
}
